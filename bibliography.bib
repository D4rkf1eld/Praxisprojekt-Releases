@online{tuchfabrik_von_2016,
	title = {Von der Wolle zum Garn – Teil 2 Spinnen},
	url = {https://www.youtube.com/watch?v=aOCokpSYIRo},
	abstract = {Wir sehen uns an, wie nach der Jahrhundertwende maschinell aus Schafswolle das Streichgarn für die Webstühle erzeugt wurde. Im {LVR} Industriemuseum Tuchfabrik Müller in Euskirchen können die alten Maschinen von 1897 und 1913 noch heute im Betrieb bewundert werden.
   Im zweiten Teil dieser zweiteiligen Dokumentation zeigen wir die Spinnmaschine, die aus dem Vorgarn der Krempelmaschine das Streichgarn für die Webstühle spinnt. Im Vergleich betrachten wir ein altes Handspinnrad an und suchen die Gemeinsamkeiten zur industriellen Lösung.
   Lassen Sie sich überraschen! Werden Sie zum Insider der historischen Tuchherstellung bei einem Besuch im Museum!
   Mehr zum Museum im Netz:  www.industriemuseum.lvr.de Schauplatz Euskirchen.},
	titleaddon = {{YouTube}},
	author = {{Tuchfabrik}},
	urldate = {2022-07-08},
	date = {2016-02-04},
	langid = {german},
}

